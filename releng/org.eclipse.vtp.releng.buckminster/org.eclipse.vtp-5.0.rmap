  <rm:searchPath name="OpenVXML-Platform-Features">
  	<rm:provider componentTypes="buckminster,eclipse.feature" readerType="local" resolutionFilter="">
  		<rm:uri format="{0}/openvxml,platforms/features/{1}">
  			<bc:propertyRef key="workspace.root"/>
  			<bc:propertyRef key="buckminster.component"/>
  		</rm:uri>
  		<!--<rm:property key="git.remote.uri" value="https://github.com/saisyamd/OpenVXML.git"/>
  		<rm:property key="git.branch.name" value="remove_version"/>
  		<rm:property key="git.auto.fetch" value="true"/>-->
  	</rm:provider>
  </rm:searchPath>
